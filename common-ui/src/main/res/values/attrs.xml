<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopBar">
        <attr name="leftIcon" format="color|reference" />
        <attr name="leftText" format="string"/>
        <attr name="title" format="string"/>
        <attr name="rightText" format="string"/>
        <attr name="rightIcon1" format="color|reference" />
        <attr name="rightIcon2" format="color|reference" />
        <attr name="rightIcon3" format="color|reference" />
        <attr name="leftIconMode" format="enum">
            <enum name="none" value="0" />
            <enum name="back" value="1" />
            <enum name="hide" value="2" />
        </attr>
    </declare-styleable>
</resources>